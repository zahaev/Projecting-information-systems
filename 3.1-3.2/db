CREATE TABLE patients (  
    id SERIAL PRIMARY KEY,  
    name VARCHAR(100) NOT NULL,  
    date_of_birth DATE NOT NULL,  
    email VARCHAR(100),  
    address VARCHAR(255),  
    phone VARCHAR(15)  
);  
CREATE TABLE doctors (  
    id SERIAL PRIMARY KEY,  
    name VARCHAR(100) NOT NULL,  
    specialization VARCHAR(100) NOT NULL,  
    cabimet VARCHAR(100) NOT NULL
);  
